<div class="box_info">
  <dl>
    <dt>Defined in:</dt>
    <dd>
      <%= object.file %><% if object.files.size > 1 %><span class="defines">,<br />
      <%= object.files[1..-1].map {|f| f.first }.join(",<br /> ") %></div>
      <% end %>
    </dd>
  </dl>
  <dl>
    <dt>Resource type:</dt>
    <dd><%= linkify(Registry["puppet_types::#{object.type_name}"], object.type_name) %></dd>
  </dl>
</div>
